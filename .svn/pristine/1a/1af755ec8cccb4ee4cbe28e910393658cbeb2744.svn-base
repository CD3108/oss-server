<!DOCTYPE HTML>
<html class="s_bs">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1" />
	<title>WmTabbar演示</title>
	{%>template/common/Head.html%}
	<link href="biz/css/project.css" rel="stylesheet" type="text/css" />
	<script type="text/javascript" src="biz/js/tag/WmTabbar.js"></script>
</head>
<body>

<div class="c_navBar">
	<div class="left">
		<div class="back">
			<span class="e_ico-back"></span>
			<span class="text">WmTabbar</span>
		</div>
	</div>
</div>

<wm-tabbar id="myTabbar">
	<wm-tabbar-item label="菜单1" icon="e_ico-device">
		<div class="c_submit">
			<ul>
				<li><button class="e_button-cancel" id="testAction">绑定选中事件</button></li>
			</ul>
		</div>
		<div class="c_submit">
			<ul>
				<li><button class="e_button-ok" id="testActive">切换标签页面</button></li>
			</ul>
		</div>
		<div class="c_submit">
			<ul>
				<li><button class="e_button-cancel" id="testAnimation">关闭组件动画</button></li>
			</ul>
		</div>
	</wm-tabbar-item>
	<wm-tabbar-item label="菜单2" icon="e_ico-component"  id="myTabbarItem">
		<div class="c_list">
			<ul>
				<li>
					<div class="content">
						<div class="main">菜单二内容</div>
						<div class="more"></div>
					</div>
				</li>
			</ul>
		</div>
	</wm-tabbar-item>
	<wm-tabbar-item label="菜单3" icon="e_ico-code">
		<div class="c_list">
			<ul>
				<li>
					<div class="content">
						<div class="main">菜单三内容</div>
						<div class="more"></div>
					</div>
				</li>
			</ul>
		</div>
	</wm-tabbar-item>
	<wm-tabbar-item label="菜单4" icon="e_ico-about">
		<div class="c_list">
			<ul>
				<li>
					<div class="content">
						<div class="main">菜单四内容</div>
						<div class="more"></div>
					</div>
				</li>
			</ul>
		</div>
	</wm-tabbar-item>
</wm-tabbar>
</body>
</html>