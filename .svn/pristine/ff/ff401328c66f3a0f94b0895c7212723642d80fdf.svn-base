<!DOCTYPE HTML>
<html class="s_bs">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1" />
<title>通讯录</title>
{%>template/common/Head.html%}
<script type="text/javascript" src="biz/js/plugin/contacts.js"></script>
</head>
<body>
	<div class="c_navBar">
		<div class="left">
			<div class="back">
				<span class="e_ico-back"></span> <span class="text">通讯录</span>
			</div>
		</div>
	</div>
	
	<div class="m_content m_content-nofooter" id="content">
		<div>
			<div class="c_title">详情</div>
			<div class="c_list">
				<ul>
					<li tapfor="input-text">
						<div class="content">
							<div class="label">姓名</div>
							<div class="info" id="name"></div>
						</div>
					</li>
					<li tapfor="input-text">
						<div class="content">
							<div class="label">电话</div>
							<div class="info" id="phoneNumber"></div>
						</div>
					</li>
					<li tapfor="input-text">
						<div class="content">
							<div class="label">邮件</div>
							<div class="info" id="email"></div>
						</div>
					</li>
				</ul>
			</div>
			<div class="c_submit">
				<ul>
					<li><button class="e_button-ok" id="getContactDetail">获取详情</button></li>
				</ul>
			</div>
		</div>
	</div>
</div>
</body>
</html>